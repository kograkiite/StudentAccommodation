<Window x:Class="StudentManagement.StudentDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Student Dashboard" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Register Contract" Width="150" Margin="10" Click="RegisterContract_Click"/>
            <Button Content="Room Student" Width="150" Margin="10" Click="RoomStudent_Click"/>
            <Button Content="Payment Infomation" Width="150" Margin="10" Click="Payment_Click"/>
        </StackPanel>
    </Grid>
</Window>
